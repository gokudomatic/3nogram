script/source = "extends Spatial

func _ready():
	get_tree().call_group(\"sfx\",\"set_music\",\"Dagaception.ogg\")
	pass

func _input(event):
	if event.is_action(\"break_action\"):
		var is_pressed=event.is_action_pressed(\"break_action\")
		$CanvasLayer/Container/break_button.pressed=is_pressed
		_on_break_button_toggled(is_pressed)
		
	elif event.is_action(\"paint_action\"):
		var is_pressed=event.is_action_pressed(\"paint_action\")
		$CanvasLayer/Container/paint_button.pressed=is_pressed
		_on_paint_button_toggled(is_pressed)

func start_puzzle(puzzle_data):
	game_data.game_status=game_data.PLAYING
	$Manager.size=puzzle_data.dimensions
	$Manager.data=puzzle_data.data
	$Manager.visible_hints=puzzle_data.hints
	$CanvasLayer/VBoxContainer/HBoxContainer/Clock.warning_time_limit=puzzle_data.warning_time_limit
	$CanvasLayer/VBoxContainer/HBoxContainer/Clock.gameover_time_limit=puzzle_data.gameover_time_limit
	$CanvasLayer/VBoxContainer/LifeBar.value=puzzle_data.lifes
	$Manager.generate=true
	
	$CanvasLayer/VBoxContainer/HBoxContainer/Clock.running=true
	set_process(true)

func _on_break_button_toggled(button_pressed):
	if button_pressed:
		game_data.touch_mode=game_data.TOUCH_DESTROY
		$CanvasLayer/Container/paint_button.pressed=false
	else:
		game_data.touch_mode=game_data.TOUCH_NORMAL


func _on_paint_button_toggled(button_pressed):
	if button_pressed:
		game_data.touch_mode=game_data.TOUCH_PAINT
		$CanvasLayer/Container/break_button.pressed=false
	else:
		game_data.touch_mode=game_data.TOUCH_NORMAL


func _on_Manager_mistaked():
	$CanvasLayer/VBoxContainer/LifeBar.value-=1
	if $CanvasLayer/VBoxContainer/LifeBar.value<=0:
		game_data.game_status=game_data.GAME_OVER
		$CanvasLayer/VBoxContainer/HBoxContainer/Clock.running=false
		$CanvasLayer/EndGameDialog/Label.text=\"Game over\"
		$CanvasLayer/EndGameDialog.show_modal(true)
		get_tree().call_group(\"sfx\",\"stop_music\")
		get_tree().call_group(\"sfx\",\"play\",\"gameover\")
	else:
		get_tree().call_group(\"sfx\",\"play\",\"wrong\")


func _on_Clock_timer_notification(status):
	if status==1:
		print(\"you're slow\")
	elif status==10:
		game_data.game_status=game_data.GAME_OVER
		$CanvasLayer/VBoxContainer/HBoxContainer/Clock.running=false
		$CanvasLayer/EndGameDialog/Label.text=\"Time up!\"
		$CanvasLayer/EndGameDialog.show_modal(true)
		get_tree().call_group(\"sfx\",\"stop_music\")
		get_tree().call_group(\"sfx\",\"play\",\"gameover\")

func _process(delta):
	var t=$CanvasLayer/VBoxContainer/HBoxContainer/Clock.current_time
	var seconds=int((t-int(t))*60)
	$CanvasLayer/VBoxContainer/HBoxContainer/timer.text=\"{0}:{1}\".format([\"%02d\" % t,\"%02d\" % seconds],\"{_}\")

func _on_Manager_finished():
	$CanvasLayer/VBoxContainer/HBoxContainer/Clock.running=false
	game_data.game_status=game_data.GAME_OVER
	game_data.touch_mode=game_data.TOUCH_NORMAL
	$CanvasLayer/Container.hide()
	$CanvasLayer/win_label.show()
	$CanvasLayer/WinBtn.show()
	get_tree().call_group(\"sfx\",\"stop_music\")
	get_tree().call_group(\"sfx\",\"play\",\"win\")

func _on_EndBtn_pressed():
	get_tree().call_group(\"sfx\",\"play\",\"select\")
	get_tree().change_scene(\"res://MainMenu.tscn\")


func _on_SoundBtn_toggled(button_pressed):
	if button_pressed:
		AudioServer.set_bus_mute(1,button_pressed)
		AudioServer.set_bus_mute(2,button_pressed)
	else:
		AudioServer.set_bus_mute(1,game_data.config.nosound)
		AudioServer.set_bus_mute(2,game_data.config.nomusic)


func _on_WinBtn_pressed():
	get_tree().call_group(\"sfx\",\"play\",\"select\")
	get_tree().change_scene(\"res://Evaluation.tscn\")


func _on_ExitBtn_pressed():
	get_tree().call_group(\"sfx\",\"play\",\"select\")
	$CanvasLayer/QuitDialog.popup_centered()


func _on_QuitNoBtn_pressed():
	get_tree().call_group(\"sfx\",\"play\",\"select\")
	$CanvasLayer/QuitDialog.hide()


func _on_QuitYesBtn_pressed():
	get_tree().call_group(\"sfx\",\"play\",\"select\")
	get_tree().change_scene(\"res://MainMenu.tscn\")
"

